{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "shared/*": ["shared/*"]
    },
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "isolatedModules": true
  }
}
